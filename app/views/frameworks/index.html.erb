<% content_for(:title, "Frameworks") %>
<div class="container">
  <div class="row">

    <div class="col-sm-3">
      <h1>New</h1>
      <%= flash[:notice] %>

      <form action='/frameworks/<%= @framework.id %>' method='post'>
      <%= render('form') %>
      <button>Update Now</button>
      </form>
    </div>


    <div class="col-sm-3">
      <h1>Frameworks</h1>
      <ul>
        <% @frameworks.each do |framework| %>
          <li><a href="/frameworks/<%= framework.id %>"><%= framework.name %></a></li>
        <% end %>
      </ul>
    </div>
  </div>
</div>
